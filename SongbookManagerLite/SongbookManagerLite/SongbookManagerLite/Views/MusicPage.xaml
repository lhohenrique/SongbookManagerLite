<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SongbookManagerLite.Views.MusicPage">

    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Filtro" Order="Primary"/>
        <ToolbarItem Text="+" Order="Primary" Command="{Binding NewMusic}"/>
    </ContentPage.ToolbarItems>

    <ContentPage.Content>
        <StackLayout>
            <ListView x:Name="MusicPageMusicListListView"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>